<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <PROJECT_ROOT>$(SolutionDir)..\..\..</PROJECT_ROOT>
    <EXTERN_PATH>$(PROJECT_ROOT)\Extern</EXTERN_PATH>
    <ENGINE_SOURCE_PATH>$(PROJECT_ROOT)\Radiance\Source</ENGINE_SOURCE_PATH>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>ProjectRoot</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="PROJECT_ROOT">
      <Value>$(PROJECT_ROOT)</Value>
    </BuildMacro>
    <BuildMacro Include="EXTERN_PATH">
      <Value>$(EXTERN_PATH)</Value>
    </BuildMacro>
    <BuildMacro Include="ENGINE_SOURCE_PATH">
      <Value>$(ENGINE_SOURCE_PATH)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>